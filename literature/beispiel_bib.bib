@article{h_schmid_approximating_2000,
	title = {Approximating the universal active element},
	volume = {47},
	rights = {All rights reserved},
	issn = {1558-125X},
	doi = {10.1109/82.885124},
	abstract = {The classification of universal amplifiers presented in this paper places all operational amplifiers and current conveyors known from the literature into a common framework, together with abstract concepts such as the universal active element and the nullor. Our approach is new in that we base it on four-terminal theory, which results in a classification that is more extensive but not more complex than classifications derived using two-port theory. It turns out that our classification contains a new type of operational amplifier, which we call current-feedback operational transconductance amplifier ({CFB} {OTA}), and also a new class of voltage-inverting current conveyors. We then demonstrate that our classification is very closely related to integrated-amplifier design by showing how all operational amplifiers and current conveyors can be implemented in {CMOS} using only a few {CMOS} circuits. Since the basic ideas behind {CMOS} and bipolar circuits are very similar, this paper is not process specific and can be seen as an attempt to bridge the gap between amplifier theory and amplifier design that has become ever wider in the past few years.},
	pages = {1160--1169},
	number = {11},
	journaltitle = {{IEEE} Transactions on Circuits and Systems {II}: Analog and Digital Signal Processing},
	shortjournal = {{IEEE} Transactions on Circuits and Systems {II}: Analog and Digital Signal Processing},
	author = {{H. Schmid}},
	date = {2000-11},
	keywords = {amplifier design, amplifier theory, Capacitors, Circuit simulation, Circuit synthesis, {CMOS} process, current conveyors, current-feedback operational transconductance amplifier, feedback amplifiers, Filters, four-terminal theory, Impedance, multiterminal networks, nullor, operational amplifiers, Operational amplifiers, Resistors, Transconductance, universal active element, universal amplifiers, Voltage, voltage-inverting current conveyors}
}